{
  "builds": [
    {
      "src": "api/tires.js", 
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/tires",
      "destination": "/api/tires"
    },
    {
      "source": "/",
      "destination": "/api/tires"
    },
    {
      "source": "/(.*)",
      "destination": "/api/tires"
    }
  ]
}
