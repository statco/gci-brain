{
  "builds": [
    {
      "src": "tires.js",
      "use": "@vercel/node",
      "config": {
        "memory": 256,
        "maxDuration": 30
      }
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/tires"
    },
    {
      "source": "/(.*)",
      "destination": "/tires"
    }
  ]
}
