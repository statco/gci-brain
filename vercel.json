{
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "dist",
  "functions": {
    "api/tires.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/tires",
      "destination": "/api/tires.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
